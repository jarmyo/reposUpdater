<Window x:Class="ReposUpdate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
       Height="73" Width="400" ResizeMode="NoResize" WindowStyle="None" WindowStartupLocation="CenterScreen" Background="#FF181818">
    <Grid>
        <ProgressBar x:Name="Progreso" Height="25" Margin="5,5,5,0" VerticalAlignment="Top" Foreground="#FF0689B0" BorderBrush="{x:Null}" Background="#FF202020"/>
        <TextBlock x:Name="newVersion" HorizontalAlignment="Right" Margin="0,35,10,0" Text="TextBlock" VerticalAlignment="Top" Foreground="White"/>
        <TextBlock x:Name="_tProgreso" HorizontalAlignment="Left" Margin="10,35,0,0" Text="TextBlock" VerticalAlignment="Top" Foreground="Gainsboro"/>
        <TextBlock x:Name="_tDescarga" HorizontalAlignment="Left" Margin="10,52,0,0" Text="TextBlock" VerticalAlignment="Top" Foreground="#FF959595"/>
    </Grid>
</Window>
